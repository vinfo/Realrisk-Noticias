<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns = "http://www.w3.org/ns/widgets"
    xmlns:gap = "http://phonegap.com/ns/1.0"
    id        = "com.phonegap.realrisk"
    version   = "1.0.0">
	
<preference name="phonegap-version" value="3.6.3" />

<name>RealRisk</name>
<icon src="image/icon.png" gap:platform="android" gap:density="ldpi"/>
<icon src="res/icon/icon-57.png" gap:platform="ios" width="57" height="57" />
<icon src="res/icon/icon-72.png" gap:platform="ios" width="72" height="72" />

<!-- iPhone / iPod Touch  -->
<icon src="res/icon/icon-60.png" gap:platform="ios" width="60" height="60" />
<icon src="res/icon/icon-60@2x.png" gap:platform="ios" width="120" height="120" />

<!-- iPad -->
<icon src="res/icon/icon-76.png" gap:platform="ios" width="76" height="76" />
<icon src="res/icon/icon-76@2x.png" gap:platform="ios" width="152" height="152" />

<!-- Settings Icon -->
<icon src="res/icon/icon-small.png" gap:platform="ios" width="29" height="29" />
<icon src="res/icon/icon-small@2x.png" gap:platform="ios" width="58" height="58" />

<!-- Spotlight Icon -->
<icon src="res/icon/icon-40.png" gap:platform="ios" width="40" height="40" />
<icon src="res/icon/icon-40@2x.png" gap:platform="ios" width="80" height="80" />


<gap:splash src="image/320-480.png" gap:platform="android" gap:density="ldpi"/>
<gap:splash src="image/480-320.png" gap:platform="android" gap:density="mdpi"/>
<gap:splash src="image/603-966.png" gap:platform="android" gap:density="hdpi"/>
<gap:splash src="image/966-603.png" gap:platform="android" gap:density="xhdpi"/>


<!-- iPhone and iPod touch -->
<gap:splash src="splash/ios/Default.png" gap:platform="ios" width="320" height="480" />
<gap:splash src="splash/ios/Default@2x.png" gap:platform="ios" width="640" height="960" />

<!-- iPhone 5 / iPod Touch (5th Generation) -->
<gap:splash src="splash/ios/Default-568h@2x.png" gap:platform="ios" width="640" height="1136" />

<!-- iPad -->
<gap:splash src="splash/ios/Default-Portrait.png" gap:platform="ios" width="768" height="1024" />
<gap:splash src="splash/ios/Default-Landscape.png" gap:platform="ios" width="1024" height="768" />

<!-- Retina iPad -->
<gap:splash src="splash/ios/Default-Portrait@2x.png" gap:platform="ios" width="1536" height="2048" />
<gap:splash src="splash/ios/Default-Landscape@2x.png" gap:platform="ios" width="2048" height="1536" />

<access origin="*" />

<description>
Aplicativo de consulta de noticias - RealRisk.
</description>
<preference name="permissions" value="none"/>
<author href="https://www.vinfoweb.com" email="victor.valencia@gmail.com">
Victor Valencia
</author>

</widget>
